{
  //"extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  //"settings": {
  //  "ecmascript": 6
  //},
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "forOf": true,
      "jsx": true,
      "es6": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "comma-dangle": 0,
    "indent": 0,
    "react/prop-types": 0,
    "react/jsx-indent-props": 0,
    "react/jsx-closing-bracket-location": 0,
    "object-curly-spacing": 0,
    "arrow-body-style": 0,
    "no-console": 0,
    "max-len": 0,
    "prefer-template": 0,
    "import/no-unresolved": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "semi": [
      1,
      "never"
    ],
    "no-unused-vars": [
      "warn",
      {
        "vars": "local",
        "args": "none",
        "ignoreRestSiblings": false
      }
    ],
    "no-undef": 2,
    "no-var": 1,
    "no-duplicate-case" : 2
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "__CLIENT__": true,
    "__SERVER__": true,
    "__DEVSERVER__": true,
    "__DEVCLIENT__": true,
    "Promise": true,
    "React": true,
    "ReactDOM": true,
    "Component": true,
    "PropTypes": true,
    "Link": true
  }
}
